<html>
<head>
	<meta charset="utf-8"></meta>
	<script src="jquery.js"></script>
    <script>
	var i = 0;
    $( document ).ready(function() {
        console.log( "document loaded" );
    });
 
    $( window ).load(function() {
        console.log( "window loaded" );
    });
	
	function button_onclick(){
		if (i%2==0){
			$( "h1" ).html( "hello world" );
			$( "#content" )
				.find( "h3" )
				.eq( 2 )
					.html( "new text for the third h3!" )
					.end() // Restores the selection to all h3s in #content
				.eq( 0 )
					.html( "new text for the first h3!" );
		} else {
			$( "h1" ).html( "hello jquery" );
			$( "#content" )
				.find( "h3" )
				.eq( 2 )
					.html( "a h3!" )
					.end() // Restores the selection to all h3s in #content
				.eq( 0 )
					.html( "a h3!" );
					
		}
		var strh1 = $( "h1" ).html();
		i++;
		console.log("click " + i + " " + strh1);
	}
	
    </script>
</head>
<body>
    <h1>dfaf</h1>
	<button onclick="javascript:button_onclick();" >H1CLICK</button>
	<div id="content">
		<h3>h3_0</h3>
		<h3>h3_1</h3>
		<h3>h3_2</h3>
		<h3>h3_3</h3>
		<h3>h3_4</h3>
	</div>
</body>
</html>
